{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "./src/core/*"
      ],
      "#public": [
        "./public/*"
      ],
      "#components/*": [
        "./src/core/components/*"
      ],
      "#types": [
        "./src/core/types/index.d.ts"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}